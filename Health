
import androidx.annotation.NonNull;
import androidx.annotation.Nullable;
import androidx.room.ColumnInfo;
import androidx.room.Entity;
import androidx.room.PrimaryKey;

@Entity(tableName = "healthconditions")

public class Health {


     private String hSpecialist;
     private String hDisease;

     
@PrimaryKey(autoGenerate = true)
    @NonNull
    @ColumnInfo(name = "healthId")
    private int hId;

    @Nullable
    @ColumnInfo(name = "healthName")
    private String hName;

    private String hStyle;

     public Health(@Nullable String specialist, String disease, int rating, String creationDate) {
        hSpecialist = specialist;
        hDisease = disease;
        hRating = rating;
        hCreationDate = creationDate;
    }

    public int getId() {
        return hId;
    }

    public void setId(int id) {
        hId = id;
    }

    public String getName() {
        return hName;
    }
    
 public void setName(String specialist) {
        hSpecialist = specialist;
    }
    
    public String getDisease() {
        return hDisease;
    }

     public void setDisease(String disease) {
        hDisease = disease;
    }


    public int getRating() {
        return hRating;
    }

    public void setDisease(String disease) {
        hDisease = disease;
    }
    //nao se usa acentuacao para nome de variaveis

    public String getCreationDate() {
        return hCreationDate;
    }

    public void setCreationDate(String creationDate) {
        hCreationDate = creationDate;
    }

    private int hRating;
    private String hCreationDate;

}
